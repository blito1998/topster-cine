<h1>Compras:</h1>
{{#each compras}}
<div class="card mt-2">
    <div class="card-body">
        <h3>{{_id}}</h3>
        <p><b>Filme: </b> {{filme}}</p>
        <p><b>Produtos: </b> {{produtos}}</p>
        <p><b>Valor: </b> {{valor}}</p>
    </div>
</div>
{{else}}
<h4 class="mt-4">Nenhum produto cadastrado!</h4>

{{/each}}

<h1>Promocções:</h1>
{{#each promoção}}
<div class="card mt-2">
    <div class="card-body">
        <h3>{{promocaoID}}</h3>
        <p><b>Produtos: </b> {{produtosID}}</p>
        <p><b>Valor: </b> {{valor}}</p>
    </div>
</div>
{{else}}
<h4 class="mt-4">Nenhuma promoção cadastrado!</h4>

{{/each}}